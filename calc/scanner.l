%{
#include "parser.tab.h"  // Generated by Bison
%}

%%

[0-9]+      { yylval = atoi(yytext); return NUMBER; }
"+"         { return PLUS; }
"-"         { return MINUS; }
"*"         { return TIMES; }
"/"         { return DIVIDE; }
"("         { return LPAREN; }
")"         { return RPAREN; }
[ \t\n]     ;  // Ignore whitespace
.           { yyerror("Invalid character"); }

%%

int yywrap() { return 1; }